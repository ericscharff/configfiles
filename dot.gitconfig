[alias]
	change-commits = "!f() { VAR=$1; OLD=$2; NEW=$3; shift 3; git filter-branch --env-filter \"if [[ \\\"$`echo $VAR`\\\" = '$OLD' ]]; then export $VAR='$NEW'; fi\" $@; }; f"
	lg = log --pretty=format:\"%C(yellow)%h %C(blue)%an %C(green)%s %Creset\" --graph --decorate --all
[pull]
	rebase = false
[init]
	defaultBranch = main
